<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- INHERIT FORM VIEW-->
        <record model="ir.ui.view" id="view_form_section_inherit">
            <field name="name">budget.enduser.section.inherit.form</field>
            <field name="model">budget.enduser.section</field>
            <field name="inherit_id" ref="budget_enduser.view_form_section"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="Contracts">
                        <field name="section_contract_ids"/>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>